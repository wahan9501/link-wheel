{
  "name": "Link Wheel",
  "description": "Dota2 chat wheel like wheel for open quick access links",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting", "bookmarks", "storage", "tabs"],
  "host_permissions": ["https://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "web_accessible_resources": [
    {
      "resources": ["/*.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
